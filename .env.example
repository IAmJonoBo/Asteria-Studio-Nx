# Copy to .env and adjust to your local setup.

# Pipeline output directory (defaults to apps/asteria-desktop/pipeline-results)
ASTERIA_OUTPUT_DIR=

# Optional: override pipeline config path
ASTERIA_PIPELINE_CONFIG_PATH=spec/pipeline_config.yaml

# Optional: tune normalization concurrency (default 6)
ASTERIA_NORMALIZE_CONCURRENCY=6

# Optional: remote layout inference endpoint + auth
ASTERIA_REMOTE_LAYOUT_ENDPOINT=
ASTERIA_REMOTE_LAYOUT_TOKEN=
ASTERIA_REMOTE_LAYOUT_TIMEOUT_MS=5000
